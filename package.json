{
  "name": "bugsnag-vite-plugin",
  "version": "1.0.0",
  "description": "A Vite plugin for integrating Bugsnag",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "biome format --write .",
    "check-format": "biome format .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "npm run build && npm run check-format && npm run check-exports"
  },
  "keywords": [
    "vite",
    "plugin",
    "bugsnag"
  ],
  "author": "David Ferreira <mrjdavidfg@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jdavidferreira/bugsnag-vite-plugin",
  "bugs": {
    "url": "https://github.com/jdavidferreira/bugsnag-vite-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdavidferreira/bugsnag-vite-plugin.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@biomejs/biome": "^1.9.4",
    "typescript": "^5.8.3"
  }
}
